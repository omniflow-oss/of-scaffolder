{
  "schemaVersion": 1,
  "groupId": "{{groupId}}",
  "platformArtifactId": "{{platformArtifactId}}",
  "platformVersion": "{{platformVersion}}",
  "versions": {
    "java": "{{javaVersion}}",
    "quarkusPlatform": "{{quarkusPlatformVersion}}",
    "quarkusPlatformGroupId": "{{quarkusPlatformGroupId}}",
    "quarkusPlatformArtifactId": "{{quarkusPlatformArtifactId}}",
    "mandrelBuilderImage": "{{mandrelBuilderImage}}",
    "mavenMinVersion": "{{mavenMinVersion}}",
    "enforcerPlugin": "{{enforcerVersion}}",
    "surefirePlugin": "{{surefireVersion}}",
    "spotlessPlugin": "{{spotlessVersion}}",
    "checkstylePlugin": "{{checkstyleVersion}}",
    "spotbugsPlugin": "{{spotbugsVersion}}",
    "archunit": "{{archunitVersion}}"
  },
  "defaults": {
    "service": {
      "addWorkflows": false,
      "registerInRootPom": true,
      "internalLibs": [],
      "dockerBaseImage": "{{dockerBaseImage}}",
      "quarkusExtensions": [
        "quarkus-rest",
        "quarkus-rest-jackson",
        "quarkus-hibernate-validator",
        "quarkus-smallrye-health"
      ],
      "testDependencies": [
        { "groupId": "io.quarkus", "artifactId": "quarkus-junit5", "scope": "test" },
        { "groupId": "io.rest-assured", "artifactId": "rest-assured", "scope": "test" },
        { "groupId": "com.tngtech.archunit", "artifactId": "archunit-junit5", "version": "${archunit.version}", "scope": "test" }
      ]
    },
    "lib": {
      "registerInRootPom": true,
      "registerInBom": true
    }
  }
}
